{
  "name": "angular-universal-pwa",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "8.0.0"
  },
  "author": "Gustavo Zavaleta <mzavaletavargas@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mzavaletaglobant/angular-samplegi"
  },
  "scripts": {
    "start": "NODE_ENV=production && node run serve-universal",
    "test": "ng test --watch false",
    "lint": "ng lint",
    "rimraf": "rimraf",
    "postinstall": "npm run build-bundles",
    "clean:client": "npm run rimraf -- dist/browser",
    "clean:server": "npm run rimraf -- dist/server dist/server.js ",
    "serve-mean": "BUILD_TYPE=mean node dist/server.js",
    "serve-universal": "BUILD_TYPE=universal node dist/server.js",
    "watch-client": "npm run build-dev --watch",
    "watch-server": "webpack --config webpack.server.config.js --progress --colors --watch",
    "build-dev": "npm run clean:client && webpack --progress --colors -d --watch",
    "build-bundles": "npm run build-pwa && npm run build-server",
    "build-server": "npm run rimraf -- dist/server dist/server.js && ng build --prod --app 2 --output-hashing=false && webpack --config webpack.server.config.js --progress --colors",
    "build-prod": "npm run clean:client && NODE_ENV=production BUILD_AOT=1 webpack --progress --profile --bail ",
    "build-pwa": "npm run clean:client && NODE_ENV=production BUILD_AOT=1 BUILD_PWA=1 webpack --progress --profile --bail "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.10",
