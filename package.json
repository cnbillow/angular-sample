{
  "name": "angular-universal-pwa",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Gustavo Zavaleta <mzavaletavargas@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mzavaletaglobant/angular-samplegi"
  },
  "engines": {
    "node": "8.10.0",
    "npm": "5.6.0"
  },
  "scripts": {
    "ng": "ng",
    "server": "node bin/www",
    "start": "node dist/server.js",
    "postinstall": "npm run build:universal",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stats": "ng build --prod --stats-json",
    "bundle-report": "npm run stats && webpack-bundle-analyzer dist/browser/stats.json",
    "build:universal": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:server": "webpack --config webpack.server.config.js --progress --colors --watch",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "serve:universal": "node dist/server.js",
    "serve:prerenders": "node dist/prerender.js",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/platform-server": "^5.2.10",
    "@angular/router": "^5.2.10",
    "@angular/service-worker": "^5.2.10",
    "@nguniversal/express-engine": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "codelyzer": "^4.2.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "mongoose": "^5.0.13",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "rxjs": "^5.5.6",
    "ts-loader": "3.5.0",
    "tslint": "^5.9.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/node": "^9.4.6",
    "typescript": "^2.8.1",
    "webpack-bundle-analyzer": "^2.11.1"
  }
}
